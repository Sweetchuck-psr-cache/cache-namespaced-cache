{
    "name": "cache/namespaced-cache",
    "description": "A decorator that makes your cache support namespaces",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "cache",
        "psr-6",
        "namespace"
    ],
    "authors": [
        {
            "name": "Tobias Nyholm",
            "email": "tobias.nyholm@gmail.com",
            "homepage": "https://github.com/nyholm"
        }
    ],
    "homepage": "https://www.php-cache.com/en/latest/",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "repositories": {
        "cache/adapter-common": {
            "type": "github",
            "url": "https://github.com/Sweetchuck-psr-cache/cache-adapter-common.git"
        },
        "cache/array-adapter": {
            "type": "github",
            "url": "https://github.com/Sweetchuck-psr-cache/cache-array-adapter.git"
        },
        "cache/hierarchical-cache": {
            "type": "github",
            "url": "https://github.com/Sweetchuck-psr-cache/cache-hierarchical-cache.git"
        },
        "cache/integration-tests": {
            "type": "github",
            "url": "https://github.com/Sweetchuck-psr-cache/cache-integration-tests.git"
        },
        "cache/tag-interop": {
            "type": "github",
            "url": "https://github.com/Sweetchuck-psr-cache/cache-tag-interop.git"
        },
        "tedivm/stash": {
            "type": "github",
            "url": "https://github.com/Sweetchuck/tedivm-stash.git"
        }
    },
    "require": {
        "php": ">=8.1",
        "cache/hierarchical-cache": "3.x-dev",
        "psr/cache": "^3.0"
    },
    "require-dev": {
        "cache/array-adapter": "3.x-dev",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.0",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Cache\\Namespaced\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cache\\Namespaced\\Tests\\": "tests/src/"
        }
    },
    "scripts": {
        "lint": [
            "@lint:phpcs",
            "@lint:phpstan"
        ],
        "lint:phpcs": "phpcs",
        "lint:phpstan": "phpstan",
        "test:all": "phpunit",
        "test": [
            "@test:acceptance"
        ],
        "test:acceptance": "phpunit --testsuite='Acceptance'"
    }
}
